generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  secret   String @unique
  pos_x    String
  pos_y    String
  target_x String
  target_y String
  shape    String
  color    String
}
